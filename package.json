{
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "name": "rocketchat",
  "description": "Official OSX, Windows, and Linux Desktop Clients for Rocket.Chat",
  "version": "3.7.4",
  "author": "Rocket.Chat Support <support@rocket.chat>",
  "copyright": "Â© 2016-2021, Rocket.Chat",
  "homepage": "https://rocket.chat",
  "license": "MIT",
  "keywords": [
    "rocketchat",
    "desktop",
    "electron"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocketChat/Rocket.Chat.Electron.git"
  },
  "bugs": {
    "url": "https://github.com/RocketChat/Rocket.Chat.Electron/issues"
  },
  "main": "app/main.js",
  "scripts": {
    "postinstall": "run-s install-app-deps clean",
    "start": "run-s clean build:watch",
    "clean": "rimraf app dist",
    "build": "run-p .:build:rollup .:build:workspaces",
    ".:build:rollup": "rollup -c",
    ".:build:workspaces": "yarn workspaces run build",
    "release": "yarn electron-builder --publish onTagOrDraft --x64",
    "build:watch": "rollup -c -w",
    "install-app-deps": "electron-builder install-app-deps",
    "test": "xvfb-maybe jest",
    "build-assets": "ts-node -O '{\"module\":\"commonjs\"}' src/buildAssets.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "run-s .:lint:eslint .:lint:tsc",
    ".:lint:eslint": "eslint .",
    ".:lint:tsc": "tsc --noEmit --skipLibCheck",
    "lint-fix": "run-s .:lint-fix:eslint .:lint:tsc",
    ".:lint-fix:eslint": "eslint --fix ."
  },
  "dependencies": {
    "@bugsnag/js": "^7.11.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@rocket.chat/css-in-js": "^0.28.0",
    "@rocket.chat/fuselage": "^0.28.0",
    "@rocket.chat/fuselage-hooks": "^0.28.0",
    "@rocket.chat/icons": "^0.28.0",
    "abort-controller": "^3.0.0",
    "electron-dl": "^3.2.1",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.6.1",
    "i18next": "^20.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-keyed-flatten-children": "^1.3.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@fiahfy/icns-convert": "^0.0.12",
    "@fiahfy/ico-convert": "^0.0.12",
    "@jest-runner/electron": "^3.0.1",
    "@rocket.chat/eslint-config": "^0.4.0",
    "@rocket.chat/eslint-config-alt": "^0.28.0",
    "@rocket.chat/prettier-config": "^0.28.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.23",
    "@types/meteor": "^1.4.84",
    "@types/node": "^12",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.36",
